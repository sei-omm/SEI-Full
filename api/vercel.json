{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "crons": [
    {
      "path": "/api/v1/cron-job",
      "schedule": "*/2 * * * *"
    }
  ],
  "routes": [
    {
      "src": "/api(.*)",
      "dest": "src/index.ts"
    },
    {
      "src": "/(.+)",
      "dest": "public/$1"
    }
  ]
}
